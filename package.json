{
  "name": "reactClientSandbox",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "yarn build && concurrently -r 'yarn watch' 'ENVFILE=./config/.env react-native run-android'",
    "android-prod": "yarn build && ENVFILE=./config/.env react-native run-android --variant=release",
    "lint": "yarn tslint -c tslint.json 'src/**/*.tsx'",
    "watch": "yarn build -- -w",
    "build": "yarn tsc --"
  },
  "dependencies": {
    "@types/jest": "22.1.0",
    "@types/react": "16.0.35",
    "@types/react-native": "0.52.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-remove-console": "6.8.5",
    "babel-polyfill": "6.26.0",
    "concurrently": "3.5.1",
    "mobx": "3.4.1",
    "mobx-react": "4.3.5",
    "native-base": "2.3.7",
    "react": "16.2.0",
    "react-native": "0.52.2",
    "react-native-ble-manager": "6.2.6",
    "react-native-config": "^0.11.5",
    "react-native-device-brightness": "1.2.0",
    "react-native-keep-awake": "2.0.6",
    "react-native-material-dialog": "0.7.4",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "1.0.0-beta.27",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.5.0",
    "typescript": "2.6.2"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.1.4",
    "react-test-renderer": "16.2.0"
  }
}
